<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.androidtest.traincomming">

    <application
        android:name="application.MyApp"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="view.StartActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="view.activity.setinfoActivity"/>
        <activity android:name="view.activity.setaboutActivity"/>
        <activity android:name="view.activity.setadviceActivity"/>
        <activity android:name="view.activity.infologinActivity"/>
        <activity android:name="view.activity.infoavatarActivity"/>
        <activity android:name="view.activity.ticketsActivity"/>
        <activity android:name="view.activity.webViewActivity"/>
        <activity android:name="view.activity.TripActivity"/>

    </application>
    <uses-permission android:name="android.permission.INTERNET"> </uses-permission>
</manifest>